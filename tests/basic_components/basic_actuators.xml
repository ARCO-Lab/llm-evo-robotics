<mujoco model="actuators">
    <actuator>
        <!-- 1️⃣ 力控制（Motor）-->
        <motor name="motor_weak" ctrlrange="-1 1" gear="5"/>
        <motor name="motor_medium" ctrlrange="-2 2" gear="10"/>
        <motor name="motor_strong" ctrlrange="-5 5" gear="20"/>
        <motor name="motor_high_torque" ctrlrange="-10 10" gear="50"/>
        
        <!-- 2️⃣ 位置控制（Position） -->
        <position name="position_fast" joint="joint1" ctrlrange="-1 1" kp="50"/>
        <position name="position_slow" joint="joint2" ctrlrange="-1 1" kp="10"/>
        <position name="position_precise" joint="joint3" ctrlrange="-0.5 0.5" kp="100"/>
        
        <!-- 3️⃣ 速度控制（Velocity） -->
        <velocity name="velocity_weak" ctrlrange="-5 5"/>
        <velocity name="velocity_strong" ctrlrange="-10 10"/>
        <velocity name="velocity_precise" ctrlrange="-2 2"/>
        
        <!-- 4️⃣ 伺服控制（Servo） -->
        <servo name="servo_standard" joint="joint1" kp="10"/>
        <servo name="servo_fast" joint="joint2" kp="50"/>
        <servo name="servo_high_torque" joint="joint3" kp="100"/>
        
        <!-- 5️⃣ 通用力矩（General） -->
        <general name="general_linear" joint="joint1" biastype="affine" gainprm="1 0 0" biasprm="0 0 0"/>
        <general name="general_nonlinear" joint="joint2" biastype="cubic" gainprm="1 0 0 0 0" biasprm="0 0 0 0 0"/>
        
        <!-- 6️⃣ 肌肉 / 气动控制（Muscle） -->
        <muscle name="muscle_weak" tendon="tendon1" ctrlrange="0 1" scale="50"/>
        <muscle name="muscle_strong" tendon="tendon2" ctrlrange="0 1" scale="100"/>
    </actuator>
</mujoco>
